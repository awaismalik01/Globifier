{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "body-parser",
      "use": "cors",
      "use": "dotenv",
      "use": "express",
      "use": "mongoose",
      "use": "jsonwebtoken",
      "use": "multer",
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
